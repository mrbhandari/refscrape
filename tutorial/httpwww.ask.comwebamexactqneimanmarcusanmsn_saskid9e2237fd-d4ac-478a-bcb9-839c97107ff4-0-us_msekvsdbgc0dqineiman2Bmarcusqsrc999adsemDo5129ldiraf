                        if(typeof State.data.state === 'undefined'){
                            new Image().src = url;
                            setTimeout((function(History) {
                                return function() {
                                    History.back();
                                };
                            })(History),delay);
                        }
                    });
                }
            } catch(err){
                            }
        });
    </script>
