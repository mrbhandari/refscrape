    function _saSetTodaysOpeningHours(openingHours, targetSpanId) {
        var today = new Date();
        var weekday = today.getDay().toString();

        if (openingHours.hasOwnProperty(weekday)) {
            var openingHoursValueSpan = document.getElementById(targetSpanId);
            openingHoursValueSpan.innerHTML=openingHours[weekday];

            var openingHoursContainer = openingHoursValueSpan.parentNode;
            openingHoursContainer.className =  "sa_yelp_opening_hours_container";
        }
    }
