			$(this).filter(function() {
				//do if nothing entered
				return $(this).val() == "" || $(this).val() == "Search"
	
			}).removeClass("watermarkOn").val("");
		});
	
		$("#search_input").blur(function() {
			$(this).filter(function() {
				//do if nothing entered
				return $(this).val() == ""
	
			}).addClass("watermarkOn").val("Search");
	
		});
	});
	
	var isiPad = navigator.userAgent.match(/iPad/i) != null;
	if(isiPad){
		document.write('<style>#nav_top #search_input {width:155px;}#nav_mid .section {padding:0 8px;} #nav_short .section {padding:0 11px !important;} #nav_mid #more_active {right:0;} #nav_bot_flex, #nav #subHeadWidgets, .short_bg_wrapper .short_bg, #nav_shade {width:105%} #nav_mid .nav_shows {padding-right:23px}</style>');
	}

</script>
