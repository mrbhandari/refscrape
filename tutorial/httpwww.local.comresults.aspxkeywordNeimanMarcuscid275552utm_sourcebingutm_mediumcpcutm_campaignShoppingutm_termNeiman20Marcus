<script type="text/javascript">
    
    var _running = false;
    function google_rs_request_done(rs) {
        if (_running) {
            return;
        }
        _running = true;

        function buildUrl(url, lunit) {
            url = decodeURIComponent(url); 
            var tm = !!lunit.term ? lunit.term : '';
            var tk = !!lunit.token ? lunit.token : '';
            if (tm == '' || tk == '')
                return url;

            url = $_url.AddParam(url, 'keyword', tm);
            url = $_url.AddParam(url, 'afdt', tk);

            return url;
        }
        var obj = $_dom.ById('ctl01_sct_ctl01_sct_ctl23');
        if (!!rs.link_units) {
            var c = $_dom.Elem('div', {}, {}, obj);
            
            var html = '<strong>Related Searches</strong>: ';
            for (var i = 0; i < rs.link_units.length; i++) {
                
                html += '<a href="' + buildUrl(rs.url, rs.link_units[i]) + '">' + rs.link_units[i].term + '</a> ';
            }
            c.innerHTML = html;
        }
        
    }
    
    var _grsOmnProps = {};
        _grsOmnProps["s.prop17"] = 'SERP';
        _grsOmnProps["s.eVar17"] = 'SERP';
