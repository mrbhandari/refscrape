	<script>
			var tapadCookieName = "SESSION_TA";
			var tapadCookie = null;
			var allCookies = document.cookie.split(';');
			for(var i=0;i < allCookies.length;i++) {
				var c = allCookies[i];
				while (c.charAt(0)==' ') c = c.substring(1,c.length);
				if (c.indexOf(tapadCookieName) == 0) {
					tapadCookie = c.substring(tapadCookieName.length,c.length);
				}
			}
			if (tapadCookie == null) {
 		jQuery(document).ready(function(){
 			var tapadUrl = "http://tapestry.tapad.com/tapestry/1?ta_partner_id=510&ta_partner_user_id={%22neiman_marcus_user_id%22:%227832892422%22}&ta_get_data=neiman_marcus_user_id&ta_callback=?";
