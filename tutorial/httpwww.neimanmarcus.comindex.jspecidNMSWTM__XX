function popUp(url, width, height, scrolls) 
{
		winOptions = "toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=" 
			+ scrolls 
			+ ",resizable=yes,copyhistory=no,width=" + width 
			+ ",height=" + height 
			+ ",screenX=20,screenY=20";
		var win = window.open(url, '', winOptions);
		if (!win.opener) win.opener = self; // For JS1.0
		if (win.focus) win.focus();
}
</SCRIPT>

