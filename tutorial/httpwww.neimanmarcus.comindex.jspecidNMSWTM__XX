<script type="text/javascript">
var intlFreeFreeAddClassCounter = 0;
function intlFreeFreeAddClass(){
	var $imgs = jQuery('.navaux img[src="/category/event/FreeFreeNavaux_1013/navaux_101813.gif"]');
	if($imgs.length > 0){
		$imgs.addClass('intlHideFreeFree');
	} else if(intlFreeFreeAddClassCounter < 20){
		intlFreeFreeAddClassCounter++;
		setTimeout("intlFreeFreeAddClass()", 100);
	}
}
if(jQuery('body').hasClass('intl')){
	intlFreeFreeAddClass();
}
</script>

<!-- START: temporary hide JEWELRY silo-->
<style type="text/css"><!--
body.intl.RussianFederation #silo5{display:none;}
body.intl.NewZealand #silo5{display:none;}
body.intl.RussianFederation.jewelry div.catalognav{visibility:hidden;}
body.intl.NewZealand.jewelry div.catalognav{visibility:hidden;}
--></style>
<script type="text/javascript">
TT_getCountryName = function(el) {
    return el.clone().children().remove().end().text();
};

var jewelryAddClassCounter = 0;
function jewelryAddClass(){
	if (jQuery('body').hasClass('RussianFederation') || jQuery('body').hasClass('NewZealand')) {
		if (window.location.href.indexOf("cat4870731") !== -1) {
			jQuery('body').addClass("jewelry");
		} else {
			var bchomeHref = jQuery("#bchome");
			if(bchomeHref.length > 0){
			    if(bchomeHref.attr('href').indexOf("cat4870731") > -1){
			        jQuery('body').addClass("jewelry");
			    }
			} else if(jewelryAddClassCounter < 10){
				jewelryAddClassCounter++;
				setTimeout("jewelryAddClass()", 100);
			}
		}
	}
}

var countryAddClassCounter = 0;
function countryAddClass(){
	var country = jQuery('#acctlink span.country-name');
	if(country.length == 0){
	   country = jQuery('#changeCountryLink span.country-txt');
	}
	if(country.length > 0){
		country = TT_getCountryName(country.first()).replace(/ /g,"");
		jQuery('body').addClass(country);
		jewelryAddClass();
	} else if(countryAddClassCounter < 20){
		countryAddClassCounter++;
		setTimeout("countryAddClass()", 100);
	}
}
if(jQuery('body').hasClass('intl')){
	countryAddClass();
}
</script>
<!-- END: temporary hide JEWELRY silo-->

	



	
		










	
	













