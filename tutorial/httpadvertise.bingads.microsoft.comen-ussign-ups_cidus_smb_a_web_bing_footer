            CrossBrowser_AttachLoadEvent(function()
            {
            var uuid="";
            var default_uuid="E50BD518-8110-48DE-B321-A72C68B382F1";
            var autoPlay=false;

            var hashCode = window.location.hash.replace('#', '').toLowerCase();
            var temp_uuid = "";
            if (hashCode.indexOf("vid=") >= 0) {
            temp_uuid = hashCode.replace('vid=', '');
            }
            if(temp_uuid==''){temp_uuid=uuid;}
            if(temp_uuid!=''){
            uuid=temp_uuid;
            autoPlay=true;
            }
            else{
            uuid=default_uuid;
            }
            VideoPlayer_BAHP_4_Render(autoPlay, uuid);
            });
            //-----------------------------------------
            function VideoPlayer_BAHP_4_Get(uuid)
            {
            window.location.hash = "vid=" + uuid;
            VideoPlayer_BAHP_4_Render(true, uuid);
            }
            //-----------------------------------------
            function VideoPlayer_BAHP_4_Render(autoPlay, uuid)
            {
            var uniqueId = "BAHP_4";
            var playerid="VideoPlayer_"+uniqueId;
            var width="512";
            VideoPlayer_RenderPlayer(playerid, uuid, width, autoPlay);
            }
